.quote {
    background: svg-load("quote.svg", fill=#dbe2ed, width=100%, height=100%);
    background-size: 15% 63px;
    background-repeat: no-repeat;
    padding-left: 10%;

    @include tablets {
        padding: 2%;
        background-image: none;
    }

    @include phones {
        padding: 0;
    }
}